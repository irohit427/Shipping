<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
<title>Shipping</title>
<style>
form {
    width: 100%;
    margin-left: 15%;
    margin-bottom: 10%;
    margin-top: 5%;
}
</style>
<% include menu %>
<div class="container">
    <h1 class="text-center">Shipping Form</h1>
    <form class="form-horizontal col-sm-10" method="post" action="/">
    <div class="form-group">
        <label class="control-label col-sm-3">Form ID:</label>
        <div class="col-sm-4">
            <input type="text" class="form-control" id="form_id" name = "form_id" placeholder="Enter Form ID">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-3">Date:</label>
        <div class="col-sm-4">
            <input type="date" class="form-control" id="date" name = "date" placeholder="Enter Date">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-3">Vessel Name:</label>
        <div class="col-sm-4">
            <input type="text" class="form-control" id="vessel" name = "vessel" placeholder="Enter Vessel Name">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-3">Group:</label>
        <div class="col-sm-4">
            <input type="text" class="form-control" id="group" name="group" placeholder="Enter Group Name">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-3">Type:</label>
        <div class="col-sm-4">
            <input type="text" class="form-control" id="type" name="type" placeholder="Enter Type">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-3">Country:</label>
        <div class="col-sm-4">
            <input type="text" class="form-control" id="country" name="country" placeholder="Enter Country">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-3">Port:</label>
        <div class="col-sm-4">
            <input type="text" class="form-control" id="port" name="port" placeholder="Enter Port">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-3">Insp. for Oil Major:</label>
        <div class="col-sm-4">
            <input type="text" class="form-control" id="insp" name="insp" placeholder="Enter Insp. for Oil Major">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-3">Name of company Inspector:</label>
        <div class="col-sm-4">
            <input type="text" class="form-control" id="company_insp" name="company_insp" placeholder="Enter Name of Company Inspector belongs to">
        </div>
    </div>
    <h4>Inspector</h4> 
    <div class="form-group">
        <label class="control-label col-sm-3">First Name</label>
        <div class="col-sm-4">
            <input type="text" class="form-control" id="insp_fn" name="insp_fn" placeholder="Enter First Name">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-3">Last Name</label>
        <div class="col-sm-4">
            <input type="text" class="form-control" id="insp_ln" name="insp_ln" placeholder="Enter Last Name">
        </div>
    </div>
    <h4>Master</h4> 
    <div class="form-group">
        <label class="control-label col-sm-3">First Name</label>
        <div class="col-sm-4">
            <input type="text" class="form-control" id="master_fn" name="master_fn" placeholder="Enter First Name">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-3">Last Name</label>
        <div class="col-sm-4">
            <input type="text" class="form-control" id="master_ln" name="master_ln" placeholder="Enter Last Name">
        </div>
    </div>
    <h4>Chief Eng.</h4> 
    <div class="form-group">
        <label class="control-label col-sm-3">First Name</label>
        <div class="col-sm-4">
            <input type="text" class="form-control" id="cheng_fn" name="cheng_fn" placeholder="Enter First Name">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-3">Last Name</label>
        <div class="col-sm-4">
            <input type="text" class="form-control" id="cheng_ln" name="cheng_ln" placeholder="Enter Last Name">
        </div>
    </div>
    <h4>Chief Officer</h4> 
    <div class="form-group">
        <label class="control-label col-sm-3">First Name</label>
        <div class="col-sm-4">
            <input type="text" class="form-control" id="choff_fn" name="choff_fn" placeholder="Enter First Name">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-3">Last Name</label>
        <div class="col-sm-4">
            <input type="text" class="form-control" id="choff_ln" name="choff_ln" placeholder="Enter Last Name">
        </div>
    </div>
    <h4>A.E</h4>
    <div class="form-group">
        <label class="control-label col-sm-3">First Name</label>
        <div class="col-sm-4">
            <input type="text" class="form-control" id="ae_fn" name="ae_fn" placeholder="Enter First Name">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-3">Last Name</label>
        <div class="col-sm-4">
            <input type="text" class="form-control" id="ae_ln" name="ae_ln" placeholder="Enter Last Name">
        </div>
    </div>
    <br>
    <div class="col text-center">
    <button type="submit" class="btn btn-primary btn-lg">Submit</button>
    </div>
    </form>
</div>
